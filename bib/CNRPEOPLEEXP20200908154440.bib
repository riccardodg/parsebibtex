@inproceedings{CNRPRODOTTI84729,
author={Del Gratta R.; Bartolini R.; Caselli T.; Monachini M.; Soria C.; Calzolari N.},
title={UFRA: a UIMA-based Approach to Federated Language Resource Architecture},
year={2008},
